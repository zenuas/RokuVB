sub co(flag: Int) [String]
	var one = 1
	yield("one")
	if flag == 0
		
		# ifスコープ外の変数(one)と同名変数定義
		var one = 3
		yield("two")
		yield(one.ToString())
	else
		
		# ifスコープ外の変数(one)とifスコープ内の変数(two)と関数引数(flag)の参照
		var two = 2
		yield("three")
		yield((one + two + flag).ToString())

var xs = co(0)

#=>one
#=>two
#=>3
xs.each(=> print($1))

var xs2 = co(4)

#=>one
#=>three
#=>7
xs2.each(=> print($1))
